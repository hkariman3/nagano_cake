<p id="blank"></p>
<div class="container">
  <div class="row">
    <div class="col-2">
  <h5 class="mt-5 mb-5 bg-secondary">管理者ログイン</h5>
    </div>
  </div>
</div>
      <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
      <div class="row">
          <div class ="col-md-2 offset-md-3">
           <%= f.label :email,"メールアドレス" %><br />
          </div>
          <div class ="col-md-4">
           <%= f.email_field :email,class:'form-control', autofocus: true, autocomplete: "email" %>
          </div>
      </div>
        <div class="row mt-3">
          <div class ="col-md-2 offset-md-3">
            <%= f.label :password,"パスワード" %><br />
          </div>
          <div class ="col-md-4">
            <%= f.password_field :password,class:'form-control', autocomplete: "current-password" %>
          </div>
        </div>


  <div class="actions">
    <%= f.submit "ログイン", class:"btn btn-primary offset-md-5 mt-4" %>
  </div>
     </div>
　      <% end %>
      </div>
  </div>
<%= render "admin/shared/links" %>


